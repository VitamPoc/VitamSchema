<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:clm68249b="urn:un:unece:uncefact:codelist:standard:UNECE:TransportEquipmentOperationalStatusCode:D12B" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:standard:UNECE:TransportEquipmentOperationalStatusCode:D12B" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
<!-- ======================================================================= -->
<!-- ===== Element Declarations                                        ===== -->
<!-- ======================================================================= -->
<!-- ===== Root Element                                                ===== -->
<!-- ======================================================================= -->
  <xsd:element name="TransportEquipmentOperationalStatusCode" type="clm68249b:TransportEquipmentOperationalStatusCodeContentType"/>
<!-- ================================================================== -->
<!-- ===== Type Definitions                                       ===== -->
<!-- ================================================================== -->
<!-- ===== Type Definition: TransportEquipmentOperationalStatusCodeContentType ===== -->
<!-- ================================================================== -->
  <xsd:simpleType name="TransportEquipmentOperationalStatusCodeContentType">
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="4">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Remain on board</ccts:Name>
            <ccts:Description>Transport equipment arriving on a marine vessel is to remain on board.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="5">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Shifter</ccts:Name>
            <ccts:Description>Transport equipment is to be shifted from one stowage location on a marine vessel to another on the same vessel.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="10">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Positioning</ccts:Name>
            <ccts:Description>Equipment is being transported for positioning purposes.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="11">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Delivery</ccts:Name>
            <ccts:Description>Equipment is being delivered.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="12">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Redelivery</ccts:Name>
            <ccts:Description>Equipment is being redelivered.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="13">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Repair</ccts:Name>
            <ccts:Description>The equipment is for repair.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="14">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Reloader</ccts:Name>
            <ccts:Description>Transport equipment to be discharged and subsequently reloaded on the same means of transport but in a different stowage location.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="15">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Returned</ccts:Name>
            <ccts:Description>The equipment is returned.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="16">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Dropped off</ccts:Name>
            <ccts:Description>The equipment is dropped off.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="17">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cross terminal transshipment</ccts:Name>
            <ccts:Description>The transport equipment that is unloaded will be loaded in another vessel at another terminal in the same port.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="18">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Booking confirmed</ccts:Name>
            <ccts:Description>Notification of the confirmation of booking of a transport equipment.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="19">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Inspected at terminal gate</ccts:Name>
            <ccts:Description>Notification that a transport equipment has undergone a terminal gate inspection.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="20">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Arrived at offloading location</ccts:Name>
            <ccts:Description>Notification that a transport equipment arrived at the offloading location.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="21">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Departed from loading location.</ccts:Name>
            <ccts:Description>Notification that a transport equipment departed from the loading location.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="22">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Loaded</ccts:Name>
            <ccts:Description>Notification that a transport equipment has been loaded.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="23">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Unloaded</ccts:Name>
            <ccts:Description>Notification that a transport equipment has been unloaded.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="24">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Intra-terminal movement</ccts:Name>
            <ccts:Description>Notification that a transport equipment has been subject to an intra-terminal movement.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="25">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Stuffing ordered</ccts:Name>
            <ccts:Description>Notification that a transport equipment has been ordered to be stuffed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="26">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Stripping ordered</ccts:Name>
            <ccts:Description>Notification that a transport equipment has been ordered to be stripped.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="27">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Stuffing confirmed</ccts:Name>
            <ccts:Description>Notification that a transport equipment has been confirmed as stuffed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="28">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Stripping confirmed</ccts:Name>
            <ccts:Description>Notification that a transport equipment has been confirmed as stripped.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="29">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Sent for heavy repair</ccts:Name>
            <ccts:Description>Notification that a transport equipment has been sent for heavy repair.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
