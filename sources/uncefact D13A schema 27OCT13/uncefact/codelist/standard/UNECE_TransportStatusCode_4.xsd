<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:clm6Recommendation24="urn:un:unece:uncefact:codelist:standard:UNECE:TransportStatusCode:4" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:standard:UNECE:TransportStatusCode:4" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
<!-- ======================================================================= -->
<!-- ===== Element Declarations                                        ===== -->
<!-- ======================================================================= -->
<!-- ===== Root Element                                                ===== -->
<!-- ======================================================================= -->
  <xsd:element name="TransportStatusCode" type="clm6Recommendation24:TransportStatusCodeContentType"/>
<!-- ================================================================== -->
<!-- ===== Type Definitions                                       ===== -->
<!-- ================================================================== -->
<!-- ===== Type Definition: TransportStatusCodeContentType        ===== -->
<!-- ================================================================== -->
  <xsd:simpleType name="TransportStatusCodeContentType">
    <xsd:restriction base="xsd:token">
      <xsd:enumeration value="1">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Arrival, completed</ccts:Name>
            <ccts:Description>The goods/consignment/equipment/means of transport has arrived.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="2">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Loading, authorized</ccts:Name>
            <ccts:Description>Authorisation to load has been given.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="3">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Arrival, in defective condition</ccts:Name>
            <ccts:Description>The goods/consignment/equipment or a means of transport has arrived in a defective condition.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="4">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Defective equipment returned to service</ccts:Name>
            <ccts:Description>The equipment previously the subject of a 'defective condition' status has been returned to service.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="5">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Process, begun</ccts:Name>
            <ccts:Description>The process has begun.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="6">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Booking, completed</ccts:Name>
            <ccts:Description>The goods/consignment/equipment or means of transport has been booked.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="7">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Booking, cancelled</ccts:Name>
            <ccts:Description>The booking of goods/consignment/equipment or means of transport has been cancelled.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="8">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cleared, import restrictions</ccts:Name>
            <ccts:Description>The goods/consignment/equipment/means of transport held for import restriction purposes, has been released for import.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="9">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cleared, export restrictions</ccts:Name>
            <ccts:Description>The goods/consignment/equipment/means of transport held for export restriction purposes, has been released for export.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="10">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cleared, by agriculture, food or fisheries authorities</ccts:Name>
            <ccts:Description>The goods/consignment/equipment/means of transport has been cleared by agriculture, food or fisheries authorities.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="11">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cleared, by port authority</ccts:Name>
            <ccts:Description>The goods/consignment/equipment/means of transport has been cleared by port authority.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="12">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Cleared, by customs</ccts:Name>
            <ccts:Description>The goods/consignment/equipment/means of transport has been cleared by customs.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="13">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Collection/pick-up, completed</ccts:Name>
            <ccts:Description>The goods/consignment/equipment has been collected/picked-up.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="14">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Process, completed</ccts:Name>
            <ccts:Description>The process has been completed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="15">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Consolidated</ccts:Name>
            <ccts:Description>The goods/consignments have been consolidated.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="16">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Crossed border</ccts:Name>
            <ccts:Description>The goods/consignments/equipment/means of transport has crossed a border.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="17">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Customs clearance, refused</ccts:Name>
            <ccts:Description>The goods/consignment/equipment/means of transport has been refused to be cleared by the customs authorities.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="18">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Damaged in the course of transportation</ccts:Name>
            <ccts:Description>The goods/consignment/equipment has been damaged in the course of transportation.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="19">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Equipment, damage quoted for</ccts:Name>
            <ccts:Description>Damaged equipment has been assessed and a repair quotation has been sent.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="20">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Delayed, in the course of transportation</ccts:Name>
            <ccts:Description>The goods/consignment/equipment/means of transport has been delayed in the course of transportation.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="21">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Delivery, completed</ccts:Name>
            <ccts:Description>The goods/consignment/equipment has been delivered.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="22">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Delivery, completed as per instruction</ccts:Name>
            <ccts:Description>Delivery of the goods/consignment/equipment has been completed as per instruction.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="23">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Delivery, not completed</ccts:Name>
            <ccts:Description>Delivery of the goods/consignment/equipment has not been completed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="24">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Departure, completed</ccts:Name>
            <ccts:Description>The means of transport has departed.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="25">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Departure, delayed</ccts:Name>
            <ccts:Description>The means of transport has been delayed in departure.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="26">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Deramped</ccts:Name>
            <ccts:Description>The equipment has been removed from a position above another type of equipment and/or on a means of transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="27">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Despatch, completed</ccts:Name>
            <ccts:Description>The goods/consignment/equipment has been despatched.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="28">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Stripped</ccts:Name>
            <ccts:Description>The goods/consignment/equipment has been unloaded from a piece of equipment in which they were transported.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="29">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Unloaded</ccts:Name>
            <ccts:Description>The goods/consignment/equipment has been unloaded from a means of transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="30">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Empty on inspection</ccts:Name>
            <ccts:Description>The package/equipment has been found to be empty on inspection.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="31">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>En route</ccts:Name>
            <ccts:Description>The goods/consignment/equipment/means of transport is moving to destination.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="32">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Equipment, returned from repair</ccts:Name>
            <ccts:Description>The equipment has been returned from repair.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="33">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Equipment, sent for repair</ccts:Name>
            <ccts:Description>The equipment has been sent for repair.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="34">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Equipment, repaired</ccts:Name>
            <ccts:Description>The equipment has been repaired.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="35">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Forwarded to destination</ccts:Name>
            <ccts:Description>The goods/consignment/equipment has been forwarded to a destination.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="36">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Not found</ccts:Name>
            <ccts:Description>The goods/consignment/equipment/means of transport has not been found.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="37">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Found</ccts:Name>
            <ccts:Description>The goods/consignment/equipment/means of transport has been found.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="38">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Freight paid</ccts:Name>
            <ccts:Description>The freight charges have been paid.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="39">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Released from bond</ccts:Name>
            <ccts:Description>The goods/consignment/equipment has been released from bond.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="40">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Arrival, at port</ccts:Name>
            <ccts:Description>The goods/consignment/equipment has arrived at the port.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="41">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Handed over under continued responsibility</ccts:Name>
            <ccts:Description>The goods/consignment/equipment has been handed over under responsibility of the same transport operator.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="44">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Misrouted consignment corrected</ccts:Name>
            <ccts:Description>The goods/consignment/equipment/means of transport has been re-routed to the correct destination.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="45">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Arrival, consignee informed</ccts:Name>
            <ccts:Description>The consignee has been informed of the arrival of the goods/consignments/equipment/means of transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="46">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Moved, into bond</ccts:Name>
            <ccts:Description>The goods/consignment/equipment has been moved into bond.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="47">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Moved, into packing depot</ccts:Name>
            <ccts:Description>The goods/consignment/equipment has been moved into a packing depot.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="48">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Loading, completed onto a means of transport</ccts:Name>
            <ccts:Description>The goods/consignment/equipment has been loaded onto a means of transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="49">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Lost</ccts:Name>
            <ccts:Description>The goods/consignment/equipment/means of transport has been lost .</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="50">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Manifested</ccts:Name>
            <ccts:Description>The goods/consignment/equipment has been included on a manifest .</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="51">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Onloaded from a means of transport</ccts:Name>
            <ccts:Description>The consignment has been onloaded from a means of transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="53">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Collection/pick-up, not completed</ccts:Name>
            <ccts:Description>The goods/consignment/equipment expected has not been collected/picked-up.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="54">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Unidentifiable</ccts:Name>
            <ccts:Description>The goods/consignment/equipment/means of transport cannot be identified.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="57">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Not loaded onto a means of transport</ccts:Name>
            <ccts:Description>The goods/consignment/equipment has not been loaded onto a means of transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="58">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Equipment, off hire</ccts:Name>
            <ccts:Description>The equipment is no longer under lease.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="59">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Premature onloading</ccts:Name>
            <ccts:Description>The goods/consignment/equipment has been onloaded prematurely from a means of transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="60">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Equipment, on hire</ccts:Name>
            <ccts:Description>The equipment has been leased.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="61">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Outstanding claims settled</ccts:Name>
            <ccts:Description>Outstanding claims in respect of the goods/consignment/equipment have been settled.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="62">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Overlanded</ccts:Name>
            <ccts:Description>Unmanifested goods/consignments/equipment has been unloaded.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="63">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Not available for collection/pick-up</ccts:Name>
            <ccts:Description>The goods/consignment/equipment is not available for collection/pick-up.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="64">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Collection/pick-up, awaited</ccts:Name>
            <ccts:Description>The goods/consignment/equipment is awaiting collection/pick-up.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="65">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Equipment, plugged-in</ccts:Name>
            <ccts:Description>The equipment has been plugged into a power source.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="66">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Plundered</ccts:Name>
            <ccts:Description>The goods/consignment/equipment/means of transport has been plundered.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="67">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Positioned</ccts:Name>
            <ccts:Description>The goods/consignment/equipment has been positioned .</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="68">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Pre-informed</ccts:Name>
            <ccts:Description>The party has been informed  in advance.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="69">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Disposed of</ccts:Name>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="70">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Ramped equipment</ccts:Name>
            <ccts:Description>The equipment has been placed above another type of equipment and/or placed on a means of transport.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="71">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Ready for transportation</ccts:Name>
            <ccts:Description>The goods/consignment/equipment is ready for transportation.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="72">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Receipt of goods fully acknowledged</ccts:Name>
            <ccts:Description>The receipt of goods has been fully acknowledged.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="73">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:Name>Receipt of goods partially acknowledged</ccts:Name>
            <ccts:Description>The receipt of goods has been partially acknowledged.</ccts:Description>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
